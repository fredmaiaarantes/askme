{
  "plugins": [
    [
      "@babel/plugin-transform-react-jsx",
      {
        "runtime": "automatic"
      }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "@/shared": "/src/shared",
          "@/private": "/private",
          "@/public": "/public",
          "@/api": "/src/api",
          "@/tests": "/src/tests",
          "@/ui": "/src/ui"
        },
        "loglevel": "silent"
      }
    ]
  ]
}